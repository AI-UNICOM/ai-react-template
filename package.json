{
  "name": "ai-unicom-cli",
  "version": "1.0.0",
  "description": "亚信联通事业部前端自动化构建框架",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint **/*.js",
    "lint-html": "eslint **/*.js -f html -o ./reports/lint-results.html",
    "lint-fix": "eslint --fix **/*.js",
    "dev": "./node_modules/.bin/webpack  --progress --colors  --config ./webpack.dev.js",
    "dist": "npm run clean-build && ./node_modules/.bin/webpack --progress --colors --bail --config ./webpack.dist.js",
    "start": "./node_modules/.bin/webpack-dev-server --config ./webpack.dev.js --open --hot",
    "clean-build": "node configures/clear.build.js",
    "clean-node_modules": "node configures/clear.node_modules.js"
  },
  "Config": {
    "host": "localhost",
    "port": 3000
  },
  "keywords": [
    "webpack",
    "jquery",
    "avalon",
    "manypage"
  ],
  "author": "jierhuofu",
  "license": "ISC",
